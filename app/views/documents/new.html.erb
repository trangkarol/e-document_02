    <div class="row">
      <%= form_for @document, url: user_documents_path, method: :post, class: "form-horizontal", multipart: true do |f| %>
        <%= render "shared/error_messages", object: f.object %>
        <%= render "document_form", f: f %>
      <% end %>
    </div>
  </div>
</div>
<div class="col-lg-4 col-md-4">
  <%= render "home_pages/list_friends_request" %>
