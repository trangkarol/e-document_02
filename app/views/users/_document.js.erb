$(document).ready(function () {
  $(document).on("click", "#up_load_file", function () {
    getNewDocument();
  });
});

function getNewDocument() {
  $.ajax({
    type: "GET",
    url: "<%= document_new_path %>",
    dataType: "json",
    success:function(data) {
      $("#div-show-user").empty();
      $("#div-show-user").html(data.html);
    }
  });
}
