$(document).ready(function () {
  $(document).on("click", "#up_load_file", function () {
    getNewDocument();
  });

  $(document).on("click", "#buton-upload-file", function () {
    alert("sdsd");
    postCreateDocument();
  });
});

function getNewDocument() {
  $.ajax({
    type: "GET",
    url: "<%= documents_new_path %>",
    dataType: "json",
    success:function(data) {
      $("#div-show-user").empty();
      $("#div-show-user").html(data.html);
    }
  });
}

